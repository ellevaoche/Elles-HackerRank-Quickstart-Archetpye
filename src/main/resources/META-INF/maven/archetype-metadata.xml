<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" name="elle-hackerrank-quickstart-archetype"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
   <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/test/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>README.md</include>
        <include>.gitignore</include>
      </includes>
    </fileSet>
  </fileSets>

  <requiredProperties>
    <requiredProperty key="groupId">
      <defaultValue>com.hackerrank</defaultValue>
    </requiredProperty>
    <requiredProperty key="artifactId">
      <defaultValue>mySolution</defaultValue>
    </requiredProperty>
    <requiredProperty key="version">
      <defaultValue>1.0.0-SNAPSHOT</defaultValue>
    </requiredProperty>
  </requiredProperties>
  
</archetype-descriptor>
